{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "bundler",
    "allowJs": false,
    "baseUrl": ".",
    "types": ["astro/client"]
  },
  "include": ["src", "astro.config.mjs", ".astro/types.d.ts"],
  "exclude": ["dist", "node_modules"]
}
